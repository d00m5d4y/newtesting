{
  "name": "Zoom Chrome Extension",
  "version": "1.8.27",
  "manifest_version": 2,
  "description": "Launches Zoom directly from Google Chrome.",
  "homepage_url": "https://zoom.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "history",
    "tabs",
    "activeTab",
    "idle",
    "bookmarks",
    "downloads",
    "storage",
    "<all_urls>"
  ]
}
